(library
 ((name ppx_core)
  (libraries (ocaml-compiler-libs.common
              compiler-libs.common
              ocaml-compiler-libs.shadow
              ppx_ast
              ppxlib.ast
              ppxlib.base
              base stdio
              ppxlib.traverse_builtins))
  (flags (:standard -open Ocaml_shadow -safe-string))
